{
  "transform": {
    "^.+\\.tsx?$": ["ts-jest", {"isolatedModules": true}]
  },
  "testEnvironment": "node",
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "testPathIgnorePatterns": [
    "\/node_modules\/",
    "\/lib\/"
  ],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
}
